<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.icia.board.dao.MemberDao">
	<select id="selectPassword" parameterType="java.lang.String" resultType="java.lang.String">
		SELECT m_pwd FROM member WHERE m_id = #{m_id}
	</select>
	<select id="selectMember" parameterType="java.lang.String" resultType="MemberDTO">
		SELECT * FROM minfo WHERE m_id=#{m_id}
	</select>
</mapper>